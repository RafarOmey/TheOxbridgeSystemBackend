    "@types/node": "^15.3.1",
    "bcrypt": "^5.0.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "date-and-time": "^1.0.1",
    "dotenv": "^8.6.0",
    "express-session": "^1.17.2",
    "generate-password": "^1.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.12.9",
    "nodemailer": "^6.6.1",
    "supertest": "^6.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
