    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/nodemailer": "^6.4.2",
    "@types/supertest": "^2.0.11",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "i": "^0.3.6",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tsc": "^1.20150623.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
